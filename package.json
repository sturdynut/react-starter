{
  "name": "react-starter",
  "version": "0.0.2",
  "author": "Matti Salokangas <matti.salokangas@gmail.com>",
  "description": "A starter kit for new React projects.",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sturdynut/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/sturdynut/react-starter/issues"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "./config/surge",
    "prebuild": "rimraf dist",
    "build": "webpack --env.prod -p",
    "postbuild": "cpy favicon.ico dist",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --env.dev",
    "postbuild:dev": "cpy favicon.ico dist",
    "start": "http-server dist",
    "dev": "webpack-dev-server --env.dev",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel lint build test",
    "lint": "eslint .",
    "setup": "yarn && npm run validate"
  },
  "dependencies": {
    "aphrodite": "^1.1.0",
    "humps": "^1.1.0",
    "lodash": "^4.16.1",
    "normalizr": "3.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5"
  },
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2016": "6.11.3",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "6.13.0",
    "babel-register": "^6.23.0",
    "chai": "3.5.0",
    "cpy-cli": "1.0.1",
    "css-loader": "^0.26.2",
    "eslint": "3.16.1",
    "eslint-config-standard": "7.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "2.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "ghooks": "1.3.2",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "inline-manifest-webpack-plugin": "3.0.1",
    "karma": "1.1.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.1.1",
    "karma-webpack": "1.7.0",
    "mocha": "3.0.1",
    "node-nightly": "^1.7.1",
    "normalize.css": "^5.0.0",
    "npm-run-all": "2.3.0",
    "offline-plugin": "3.4.2",
    "opt-cli": "1.5.1",
    "progress-bar-webpack-plugin": "1.9.3",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "2.5.4",
    "style-loader": "^0.13.2",
    "surge": "0.18.0",
    "webpack": "2.2.1",
    "webpack-config-utils": "2.0.0",
    "webpack-dev-server": "2.4.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  }
}
